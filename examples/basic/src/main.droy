// Basic Example Package for Droy
// This demonstrates a simple Droy package structure

pkg "basic-example"

// Set some variables
~s @si = "Hello"
~s @ui = "from Basic Example!"

// Main function
f main() {
    // Output greeting
    em @si + " " + @ui
    
    // Do some math
    ~s a = 10
    ~s b = 20
    ~s sum = a + b
    
    em "Sum of " + a + " and " + b + " is: " + sum
    
    // Return success
    ~r "Done!"
}

// Run main
main()
